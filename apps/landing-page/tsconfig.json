{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "types": ["node", "@docusaurus/module-type-aliases", "@docusaurus/theme-classic", "jest"],
    "esModuleInterop": true,
    "lib": ["DOM"],
    "noEmit": true,
    "noImplicitAny": true,
    "paths": {
      "@site/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
